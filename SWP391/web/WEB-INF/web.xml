<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>controller.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>testCategory</servlet-name>
        <servlet-class>controller.user.testCategory</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>filterCategory</servlet-name>
        <servlet-class>controller.user.filterCategory</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewcourse</servlet-name>
        <servlet-class>controller.user.viewCourse</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>testCategory</servlet-name>
        <url-pattern>/testCategory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>filterCategory</servlet-name>
        <url-pattern>/filterCategory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewcourse</servlet-name>
        <url-pattern>/viewcourse</url-pattern>
    </servlet-mapping>
</web-app>
